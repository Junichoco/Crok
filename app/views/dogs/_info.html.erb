<div class="dog-img">
  <div class="dog-name"><%= @dog.name %></div>
  <% if @dog.photos.attached? %>
    <%= cl_image_tag product.photos.first.key, crop: :fill %>
  <% else %>
    <%= image_tag 'dog_vet_05.jpg', alt: "default image for dog"%>
  <% end %>
  <div class="dog-score">
    <p>Score</p>
    <h4>30.0</h4>
  </div>
  <div class="medical-record-link"><i class="fa-solid fa-book-medical"></i></div>
  <div class="family-showpage-link"><i class="fa-solid fa-person person"></i><i class="fa-solid fa-dog dog"></i></div>
</div>
